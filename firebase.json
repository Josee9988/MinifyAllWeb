{
  "hosting": [
    {
      "target": "MinifyAll",
      "public": "dist/MinifyAll/dist/MinifyAll/browser",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": "ssr"
        }
      ]
    }
  ],
  "functions": {
    "source": "dist/MinifyAll"
  }
}
