{
  "name": "minifyallweb",
  "description": "Online code minifier for HTML, CSS, JSON and JS",
  "version": "1.6.9",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "postinstall": "ngcc",
    "build": "ng build --prod --optimization --common-chunk --vendor-chunk --named-chunks --aot --build-optimizer",
    "build:test": "ng build --prod --optimization --common-chunk --vendor-chunk --named-chunks --aot --build-optimizer --output-path=dist/",
    "compodoc": "compodoc src -p tsconfig.json --name 'MinifyAll Web' --output ./compodoc/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:ci": "ng test --browsers ChromeHeadless",
    "dev:ssr": "ng run MinifyAll:serve-ssr",
    "serve:ssr": "node dist/MinifyAll/server/main.js",
    "build:ssr": "ng build --prod && ng run MinifyAll:server:production",
    "prerender": "ng run MinifyAll:prerender",
    "firebase:fullDeploy": "npm run firebase:build && npm run firebase:setNodeVersionTo8 && firebase deploy",
    "firebase:build": "ng deploy || exit 0",
    "firebase:deploy:preview": "ng deploy --preview",
    "firebase:deploy:disable": "firebase hosting:disable --project minifyallweb",
    "webdriver:update": "webdriver-manager clean && webdriver-manager update",
    "firebase:setNodeVersionTo8": "cp ./dist/MinifyAll/package.json ./dist/MinifyAll/package.temp.json && node -p \"JSON.stringify({...require('./dist/MinifyAll/package.temp.json'), engines: {node: '8'}}, null, 2)\" > dist/MinifyAll/package.json && rm ./dist/MinifyAll/package.temp.json",
    "update:minifyallcore": "bash bin/coreUpdater.sh $1"
  },
  "author": {
    "name": "Jose Gracia",
    "email": "jgracia9988@gmail.com"
  },
  "maintainers": [
    {
      "email": "jgracia9988@gmail.com",
      "name": "Jose Gracia",
      "url": "https://www.npmjs.com/~josee9988"
    }
  ],
  "bugs": {
    "url": "https://github.com/Josee9988/MinifyAllWeb/issues",
    "email": "jgracia9988@gmail.com"
  },
